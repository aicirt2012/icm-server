		{
			"name": "-- Login and update token variable",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "927159e0-c7b7-4294-a350-df2906fbfe82",
						"type": "text/javascript",
						"exec": [
							"pm.test(\"Extract token from response\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.environment.set(\"token\", jsonData.token);",
							"});",
							""
						]
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n\t\"username\":\"Simon\",\n\t\"password\":\"test123\"\n}"
				},
